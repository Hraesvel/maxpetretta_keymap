// Left-hand combos
COMB(tilde,     KC_TILD,    KC_A,   KC_R)
COMB(grave,     KC_GRV,     KC_Z,   KC_X)
COMB(exclaim,   KC_EXLM,    KC_W,   KC_R)
COMB(percent,   KC_PERC,    KC_R,   KC_X)
COMB(atsign,    KC_AT,      KC_W,   KC_F)
COMB(colon,     KC_COLN,    KC_R,   KC_S)
COMB(semi,      KC_SCLN,    KC_X,   KC_C)
COMB(hash,      KC_HASH,    KC_F,   KC_S)
COMB(circ,      KC_CIRC,    KC_S,   KC_C)
COMB(lbrack,    KC_LBRC,    KC_F,   KC_P)
COMB(lparen,    KC_LPRN,    KC_S,   KC_T)
COMB(lbrace,    KC_LCBR,    KC_C,   KC_D)
COMB(dollar,    KC_DLR,     KC_P,   KC_T)
COMB(amper,     KC_AMPR,    KC_T,   KC_D)

//Right-hand combos
COMB(equal,     KC_EQL,     KC_L,       KC_N)
COMB(aster,     KC_ASTR,    KC_N,       KC_H)
COMB(rbrack,    KC_RBRC,    KC_L,       KC_U)
COMB(rparen,    KC_RPRN,    KC_N,       KC_E)
COMB(rbrace,    KC_RCBR,    KC_H,       KC_COMM)
COMB(pipe,      KC_PIPE,    KC_U,       KC_E)
COMB(langle,    KC_LABK,    KC_E,       KC_COMM)
COMB(dquote,    KC_DQUO,    KC_U,       KC_Y)
COMB(minus,     KC_MINS,    KC_E,       KC_I)
COMB(under,     KC_UNDS,    KC_COMM,    KC_DOT)
COMB(bslash,    KC_BSLS,    KC_Y,       KC_I)
COMB(rangle,    KC_RABK,    KC_I,       KC_DOT)
COMB(plus,      KC_PLUS,    KC_I,       KC_O)
COMB(question,  KC_QUES,    KC_DOT,     KC_SLSH)
